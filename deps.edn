{:deps {org.clojure/clojure {:mvn/version "1.10.3"}
        org.clojure/clojurescript {:mvn/version "1.10.879"}
        org.clojure/core.async {:mvn/version "1.3.622"}

        applied-science/js-interop {:mvn/version "0.3.1"}
        camel-snake-kebab/camel-snake-kebab {:mvn/version "0.4.2"}
        cljs-bean/cljs-bean {:mvn/version "1.7.0"}
        com.cognitect/transit-cljs {:mvn/version "0.8.269"}
        metosin/potpuri {:mvn/version "0.5.3"}
        metosin/sieppari {:mvn/version "0.0.0-alpha13"}}

 :paths ["src"]

 :aliases
 {:build
  {:jvm-opts ["-Xmx2048m"
              "-XX:+HeapDumpOnOutOfMemoryError"]
   :main-opts ["./scripts/build.clj"]}

  :outdated
  {:extra-deps {com.github.liquidz/antq {:mvn/version "RELEASE"}}
   :main-opts ["-m" "antq.core"]}

  :test
  {:extra-deps {olical/cljs-test-runner {:mvn/version "3.8.0"}}
   :extra-paths ["test"]
   :main-opts ["-m" "cljs-test-runner.main"]}

  :pack
  {:main-opts
   ["-m" "mach.pack.alpha.skinny" "--no-libs" "--project-path" "whiskey.jar"]

   :extra-deps
   {pack/pack.alpha {:git/url "https://github.com/juxt/pack.alpha.git"
                     :sha "b093f79420fef019faf62a75b888b5e10f4e8cc9"}}}}}
